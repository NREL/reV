{
  "directories": {
    "logging_directory": "TESTDATADIR/ri_pv_reV2",
    "output_directory": "TESTDATADIR/ri_pv_reV2"
  },
  "exclusions": [
    {
      "band": 1,
      "classes_exclude": [
        1
      ],
      "fpath": "/projects/rev/data/ri/ri_padus.tif",
      "name": "PAD-US",
      "type": "Protected"
    },
    {
      "band": 1,
      "classes_exclude": [
        2,
        3
      ],
      "fpath": "/projects/rev/data/ri/ri_smod.tif",
      "name": "GHSL Urban Areas",
      "type": "Urban"
    },
    {
      "band": 1,
      "fpath": "/projects/rev/data/ri/ri_srtm_slope.tif",
      "max_thresh": 5,
      "name": "SRTM Slope",
      "type": "Slope"
    }
  ],
  "exclusions_filter": "queen",
  "exclusions_output_file": "exclusions.tif",
  "exclusions_use_blocks": true,
  "execution_control": {
    "allocation": "rev",
    "nodes": 1,
    "option": "eagle",
    "walltime": 0.1
  },
  "project_control": {
    "logging_level": "DEBUG",
    "name": "rev_exclusions"
  }
}
