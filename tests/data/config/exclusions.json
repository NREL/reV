{
  "directories": {
    "logging_directory": "TESTDATADIR/ri_pv_reV2",
    "output_directory": "TESTDATADIR/ri_pv_reV2"
  },
  "dirout": "./",
  "exclusions": [
    {
      "band": 1,
      "classes_exclude": [
        1
      ],
      "fpath": "/projects/rev/data/ri/ri_padus.tif",
      "name": "PAD-US",
      "type": "Protected"
    },
    {
      "band": 1,
      "classes_exclude": [
        2,
        3
      ],
      "fpath": "/projects/rev/data/ri/ri_smod.tif",
      "name": "GHSL Urban Areas",
      "type": "Urban"
    },
    {
      "band": 1,
      "fpath": "/projects/rev/data/ri/ri_srtm_slope.tif",
      "max_thresh": 5,
      "name": "SRTM Slope",
      "type": "Slope"
    }
  ],
  "execution_control": {
    "allocation": "rev",
    "memory": 90,
    "nodes": 1,
    "option": "local",
    "walltime": 0.1
  },
  "filter": "queen",
  "fout": "exclusions.tif",
  "project_control": {
    "logging_level": "DEBUG",
    "name": "rev_exclusions"
  },
  "use_blocks": true
}
