{
  "cf_dset": "cf_mean-means",
  "data_layers": {
    "reeds_region": {
      "dset": "reeds_regions",
      "method": "mode"
    },
    "slope": {
      "dset": "srtm_slope",
      "method": "mean"
    }
  },
  "directories": {
    "logging_directory": "./logs/",
    "output_directory": "./conus_wind/"
  },
  "excl_dict": {
    "naris_wind": {
      "use_as_weights": true
    }
  },
  "excl_fpath": "/projects/rev/data/exclusions/CONUS_Exclusions.h5",
  "execution_control": {
    "allocation": "rev",
    "feature": "--qos=high",
    "memory": 90,
    "nodes": 1,
    "option": "eagle",
    "walltime": 4.0
  },
  "gen_fpath": "PIPELINE",
  "lcoe_dset": "lcoe_fcr-means",
  "power_density": 3,
  "project_control": {
    "logging_level": "INFO"
  },
  "res_class_bins": [
    0,
    8.0,
    100
  ],
  "res_class_dset": "ws_mean-means",
  "resolution": 64,
  "tm_dset": "techmap_wtk"
}
