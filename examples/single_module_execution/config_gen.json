{
  "directories": {
    "logging_directory": "./logs/",
    "output_directory": "./"
  },
  "execution_control": {
    "allocation": "rev",
    "feature": "--qos=high",
    "nodes": 5,
    "option": "eagle",
    "sites_per_worker": 100,
    "walltime": 0.5
  },
  "project_control": {
    "analysis_years": [
      2010,
      2011
    ],
    "logging_level": "INFO",
    "name": "gen",
    "output_request": [
      "cf_mean",
      "cf_profile"
    ],
    "technology": "pv"
  },
  "project_points": "./project_points/project_points.csv",
  "resource_file": "/datasets/NSRDB/v3.0.1/nsrdb_{}.h5",
  "sam_files": {
    "i_pvwatts": "./sam_configs/i_pvwatts_gen.json"
  }
}
