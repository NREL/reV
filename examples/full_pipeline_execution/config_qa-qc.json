{
  "directories": {
    "log_directory": "./logs/",
    "output_directory": "./"
  },
  "execution_control": {
    "allocation": "rev",
    "feature": "-p debug",
    "option": "eagle",
    "walltime": 1.0
  },
  "log_level": "INFO",
  "modules": {
    "collect": {
      "dsets": [
        "cf_mean",
        "ghi_mean",
        "lcoe_fcr"
      ],
      "fpath": "PIPELINE",
      "sub_dir": "QA_collect"
    },
    "exclusions": {
      "fpath": null,
      "plot_type": "plotly",
      "sub_dir": "QA_exclusions"
    },
    "multi-year": {
      "dsets": [
        "cf_mean-means",
        "cf_mean-stdev",
        "ghi_mean-means",
        "ghi_mean-stdev",
        "lcoe_fcr-means",
        "lcoe_fcr-stdev"
      ],
      "fpath": "PIPELINE",
      "plot_type": "plot",
      "sub_dir": "QA_multi-year"
    },
    "rep-profiles": {
      "dsets": [
        "rep_profiles_0"
      ],
      "fpath": "PIPELINE",
      "sub_dir": "QA_rep-profiles"
    },
    "supply-curve": {
      "fpath": "PIPELINE",
      "lcoe": "total_lcoe",
      "plot_type": "plotly",
      "sub_dir": "QA_supply-curve"
    },
    "supply-curve-aggregation": {
      "fpath": "PIPELINE",
      "plot_type": "plot",
      "sub_dir": "QA_supply-curve-aggregation"
    }
  }
}
